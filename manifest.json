{
    "manifest_version": 2,
    "name": "My Plugin",
    "version": "1.0",
    "description": "Опис мого плагіну",
    "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "browser_action": {
      "default_title": "My Plugin",
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      "index.html",
      "target/wasm32-unknown-unknown/release/my_plugin.wasm",
      "target/wasm32-unknown-unknown/release/my_plugin.js"
    ]
  }
  