{
  "name": "Hello Plugin",
	"content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-/TR/+eNA9CSpNtKtLaYtzG0kL6PWWrLgqaYi2wjAg+Y=' ; object-src 'self'",
	
  "version": "1.0",
  "manifest_version": 2,
  "description": "Displays 'Hello' message on button click",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["activeTab", "tabs", "unsafe-eval", "audioCapture", "videoCapture"],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  }
}
  